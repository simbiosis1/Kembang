<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
		font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel width="100%">
		<table>
			<tr>
				<td class="formLabel">Nama pengguna</td>
			</tr>
			<tr>
				<td>
					<g:Label ui:field="completeName" styleName="formTextlabel" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Kunci</td>
			</tr>
			<tr>
				<td>
					<g:PasswordTextBox ui:field="password" styleName="formText"
						width="200px" />
				</td>
			</tr>
			<tr>
				<td class="formLabel">Konfirmasi kunci</td>
			</tr>
			<tr>
				<td>
					<g:PasswordTextBox ui:field="passwordConfirm"
						styleName="formText" width="200px" />
				</td>
			</tr>
		</table>

	</g:HTMLPanel>
</ui:UiBinder> 