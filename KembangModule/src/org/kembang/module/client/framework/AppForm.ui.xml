<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:DockLayoutPanel unit="PX">
		<g:north size="35">
			<g:DockLayoutPanel unit="PX">
				<g:east size="150">
					<g:HorizontalPanel height="100%" width="100%"
						styleName="kembang-toolbar" horizontalAlignment="right">
						<g:HorizontalPanel ui:field="toolbarBack" height="100%" verticalAlignment="ALIGN_MIDDLE"
							horizontalAlignment="right" />
						<g:HTML>&nbsp;</g:HTML>
					</g:HorizontalPanel>
				</g:east>
				<g:center>
					<g:HorizontalPanel height="100%" width="100%"
						styleName="kembang-toolbar">
						<g:HorizontalPanel height="100%"
							verticalAlignment="ALIGN_MIDDLE">
							<g:Label ui:field="appTitle" styleName="kembang-appTitle" />
							<g:HTMLPanel styleName="kembang-appTitle">&nbsp;|&nbsp;&nbsp;</g:HTMLPanel>
							<g:HorizontalPanel ui:field="toolbar" width="100%"
								height="100%" verticalAlignment="ALIGN_MIDDLE" />
						</g:HorizontalPanel>
					</g:HorizontalPanel>
				</g:center>
			</g:DockLayoutPanel>
		</g:north>
		<g:center>
			<g:DockLayoutPanel width="100%" height="100%"
				ui:field="appPanel" />
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 